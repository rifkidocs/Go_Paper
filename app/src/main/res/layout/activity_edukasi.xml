    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="46dp"
        android:layout_height="58dp"
        android:layout_marginTop="524dp"
        android:src="@drawable/thumb2"
        app:layout_constraintEnd_toEndOf="parent"
